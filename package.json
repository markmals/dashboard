        "@remix-run/serve": "^2.9.2",
        "@vercel/remix": "^2.9.2-patch.2",
        "clsx": "^2.1.1",
        "framer-motion": "^11.2.10",
        "isbot": "^4.1.0",
        "react": "^18.2.0",
        "react-dom": "^18.2.0"
    },
    "devDependencies": {
        "@remix-run/dev": "^2.9.2",
        "@tailwindcss/aspect-ratio": "^0.4.2",
